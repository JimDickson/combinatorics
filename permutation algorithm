#algorithm to generate permutations


  
#generate array of numbers
n_array=[]
x=1
while x<n:
  n_array.append(n)
  x+=1
  
print(n_array)
 
#generate each permutation
output_permutation=""
used_array=[]

for x1 in n_array:
 output_permutation=output_permuation+x1
 n_array.append(x1)
   for x2 in n_array:
    if used_array.count(x2)<1:
     output_permutation=output_permuation+x2
     n_array.append(x2)
    for x3 in n_array:
      if used_array.count(x3)<1: 
        output_permutation=output_permuation+x3
        used_array.append(x3)
      for x4 in n_array:
